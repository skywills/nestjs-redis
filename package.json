{
  "name": "@nathapp/nestjs-redis",
  "version": "1.3.5",
  "description": "a NestJS ioRedis module",
  "author": "skunight",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skywills/nestjs-redis.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "publish-public": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint -c .eslintrc.js"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.0",
    "@types/node": "*",
    "@types/ioredis": "*",
    "@types/uuid": "*",    
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "cz-conventional-changelog": "^3",
    "eslint": "*",
    "eslint-config-prettier": "^6",
    "eslint-plugin-jest": "^24",
    "eslint-plugin-prettier": "^3",
    "jest": "^29.5.0",
    "prettier": "^2",
    "rimraf": "*",
    "ts-jest": "^29.1.0",
    "typescript": "^4"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "ioredis": "^4 || ^5",
    "redlock": "^5.0.0-beta.2",
    "rxjs": "*",    
    "uuid": "^8 || ^9"
  },
  "keywords": [
    "nestjs",
    "nest",
    "redis",
    "nestjs-redis"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "types": "./index.d.ts",
  "bugs": {
    "url": "https://github.com/skywills/nestjs-redis/issues"
  },
  "homepage": "https://github.com/skywills/nestjs-redis#readme",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  }
}
